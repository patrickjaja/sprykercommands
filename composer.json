{
  "name": "patrickjaja/sprykercommands",
  "description": "Spryker Commands",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "xervice/console": "^1.0.0",
    "phpunit/php-code-coverage": "<=5.2"
  },
  "require-dev": {
    "nexusnetsoftgmbh/nexuscli": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "SprykerCli\\": "src"
    },
    "psr-0": {
      "Nexus\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "use-include-path": true,
    "sort-packages": true,
    "github-protocols": ["https"],
    "process-timeout": 900
  }
}
